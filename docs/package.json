{
  "name": "@rarog/docs",
  "version": "0.1.0",
  "private": true,
  "description": "Документация Rarog — Docusaurus сайт (русский/английский).",
  "author": "TheSkiF4er",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheSkiF4er/rarog.git"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "docusaurus start --port 3000",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle @docusaurus/theme-classic",
    "deploy": "docusaurus deploy --out-dir build",
    "write-translations": "docusaurus write-translations --locale ru",
    "format": "prettier --write '**/*.{md,mdx,js,jsx,ts,tsx,json,css,scss}'",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src docs --max-warnings=0 || true",
    "check-links": "node ./scripts/check-links.js",
    "clean": "rimraf node_modules .docusaurus build"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.0",
    "@docusaurus/preset-classic": "^2.4.0",
    "@cmfcmf/docusaurus-search-local": "^0.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "prism-react-renderer": "^1.3.0"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0"
  },
  "browserlist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "homepage": "https://theSkif4er.github.io/rarog/",
  "keywords": [
    "rarog",
    "documentation",
    "docusaurus",
    "design-system",
    "tokens"
  ]
}
