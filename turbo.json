{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [],
      "outputs": []
    },
    "clean": {
      "cache": false,
      "outputs": []
    },
    "typecheck": {
      "dependsOn": [],
      "outputs": []
    },
    "lint": {
      "dependsOn": [],
      "outputs": []
    },
    "format": {
      "cache": false,
      "dependsOn": []
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        ".next/**",
        "lib/**",
        "out/**"
      ]
    },
    "storybook": {
      "dependsOn": [
        "^build"
      ],
      "persistent": true,
      "outputs": [
        "storybook-static/**"
      ]
    },
    "docs:dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [],
      "outputs": []
    },
    "docs:build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "docs/.docusaurus/**",
        "docs/build/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "coverage/**",
        "test-results/**"
      ]
    },
    "e2e": {
      "dependsOn": [
        "build",
        "^build"
      ],
      "outputs": [
        "e2e-results/**"
      ]
    },
    "release": {
      "dependsOn": [
        "build",
        "test",
        "docs:build"
      ],
      "cache": false,
      "outputs": []
    },
    "publish": {
      "dependsOn": [
        "release"
      ],
      "cache": false,
      "outputs": []
    },
    "analyze": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "reports/**"
      ]
    },
    "ci": {
      "dependsOn": [
        "lint",
        "typecheck",
        "test",
        "build",
        "docs:build"
      ],
      "cache": false,
      "outputs": []
    }
  },
  "globalDependencies": [
    "package.json",
    "pnpm-lock.yaml",
    "turbo.json"
  ],
  "canary": false,
  "npmClient": "pnpm",
  "pipelineDefaults": {
    "maxParallel": 6,
    "inputs": [
      "package.json",
      "tsconfig.json",
      "tsconfig.*.json",
      "pnpm-lock.yaml",
      "{projectRoot}/src/**",
      "{projectRoot}/src/**/*",
      "{projectRoot}/public/**",
      "{projectRoot}/styles/**",
      "{projectRoot}/**/*.scss",
      "{projectRoot}/**/*.css",
      "{projectRoot}/**/*.html",
      "{projectRoot}/**/*.md",
      "{projectRoot}/**/*.mdx"
    ]
  },
  "cacheOptions": {
    "remote": {
      "enabled": true
    }
  }
}
